<UserControl x:Class="Barista1.UserControls.TestBase.TextTestBase"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
			 
             xmlns:local="clr-namespace:Barista1.UserControls.TestBase"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Loaded="UserControl_Loaded">
	<Grid Background="{DynamicResource Light}">
		<ScrollViewer>
			<StackPanel x:Name="WindowContent"   Margin="0 20 0 0">
			</StackPanel>
		</ScrollViewer>
	</Grid>
</UserControl>
